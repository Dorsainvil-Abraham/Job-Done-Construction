{
    "version": 2,
    "builds": [
      {
        "src": "Home.html",
        "use": "@vercel/static"
      },
      {
        "src": "About.html",
        "use": "@vercel/static"
      },
      {
        "src": "Contact.html",
        "use": "@vercel/static"
      },
      {
        "src": "Project.html",
        "use": "@vercel/static"
      },
      {
        "src": "Service.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/Home.html"
      },
      {
        "src": "/About",
        "dest": "/About.html"
      },
      {
        "src": "/Contact",
        "dest": "/Contact.html"
      },
      {
        "src": "/Project",
        "dest": "/Project.html"
      },
      {
        "src": "/Service",
        "dest": "/Service.html"
      },
      {
        "src": "/Images/(.*)",
        "dest": "/images/$1" 
      },
      { 
        "src": "/Styles/(.*)", 
        "dest": "/styles/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }

